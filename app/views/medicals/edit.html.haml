%h1 Edit Existing Medical

= form_tag medical_path(@medical), :method => :put do

  = label :medical, :name, 'Name'
  = text_field :medical, 'name'
  %br 
  %br
  = label :medical, :review, 'Reviews'
  = text_field :medical, 'review'
  %br
  %br
  = label :medical, :address, 'Address'
  = text_field :medical, 'address'
  %br
  %br
  = label :medical, :website, 'Website'
  = text_field :medical, 'website'
  %br
  %br
  = submit_tag 'Update Medical Info'